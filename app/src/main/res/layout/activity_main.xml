                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <ImageView
                            android:contentDescription="@string/na"
                            android:src="@drawable/logout_btn"
                            android:scaleType="centerInside"
                            />
                    </TableRow>
                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="1"
