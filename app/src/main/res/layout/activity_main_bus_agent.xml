<?xml version="1.0" encoding="utf-8"?>
<tz.co.xhcodes.com.CustomRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_horizontal_margin"
    tools:context="tz.co.xhcodes.com.MainActivity"
    android:orientation="vertical"
    >
    <TableLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <TableRow
        android:id="@+id/row_one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <androidx.cardview.widget.CardView
            app:cardCornerRadius="4dp"
            android:padding="10dp"
            android:layout_marginTop="0dp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_margin="2dp"
            >

                <TableLayout
                    android:focusable="true"
                    android:clickable="true"
                    android:onClick="bookBusTicket"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <ImageView
                            android:contentDescription="@string/na"
                            android:src="@drawable/bus"
                            android:scaleType="centerInside"
                            />
                    </TableRow>

                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="1"
                            android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                            android:text="@string/bus_ticket"
                            />
                    </TableRow>
                </TableLayout>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            app:cardCornerRadius="4dp"
            android:padding="10dp"
            android:layout_marginTop="0dp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_margin="2dp"
            >
                <TableLayout
                    android:focusable="true"
                    android:clickable="true"
                    android:onClick="scanTicket"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <ImageView
                            android:contentDescription="@string/na"

                            android:src="@drawable/scan"
                            android:scaleType="centerInside"
                            />
                    </TableRow>

                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="1"
                            android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                            android:text="@string/verify_ticket"
                            />
                    </TableRow>
                </TableLayout>
        </androidx.cardview.widget.CardView>
    </TableRow>
    <TableRow
        android:id="@+id/row_two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <androidx.cardview.widget.CardView
            app:cardCornerRadius="4dp"
            android:padding="10dp"
            android:layout_marginTop="0dp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_margin="2dp"
            >

                <TableLayout
                    android:focusable="true"
                    android:clickable="true"
                    android:onClick="reprintTicket"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <ImageView
                            android:contentDescription="@string/na"

                            android:src="@drawable/print"
                            android:scaleType="centerInside"
                            />
                    </TableRow>

                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="1"
                            android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                            android:text="@string/reprint_tkt"
                            />
                    </TableRow>
                </TableLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            app:cardCornerRadius="4dp"
            android:padding="10dp"
            android:layout_marginTop="0dp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_margin="2dp"
            >

                <TableLayout
                    android:focusable="true"
                    android:clickable="true"
                    android:onClick="connectPrinter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <ImageView
                            android:contentDescription="@string/na"

                            android:src="@drawable/connect_bt"
                            android:scaleType="centerInside"
                            />
                    </TableRow>

                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="1"
                            android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                            android:text="@string/connect"
                            />
                    </TableRow>
                </TableLayout>

        </androidx.cardview.widget.CardView>
    </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView
                app:cardCornerRadius="4dp"
                android:padding="10dp"
                android:layout_marginTop="0dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_margin="2dp"
                >

                    <TableLayout
                        android:focusable="true"
                        android:clickable="true"
                        android:onClick="testPrinter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TableRow
                            android:gravity="center"
                            android:layout_weight="1">
                            <ImageView
                                android:contentDescription="@string/na"
                                android:src="@drawable/test_print"
                                android:scaleType="centerInside"
                                />
                        </TableRow>
                        <TableRow
                            android:gravity="center"
                            android:layout_weight="1">
                            <TextView
                                android:textAlignment="center"
                                android:layout_weight="1"
                                android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                                android:text="@string/test_printer"
                                />
                        </TableRow>
                    </TableLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                app:cardCornerRadius="4dp"
                android:padding="10dp"
                android:layout_marginTop="0dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_margin="2dp"
                >

                    <TableLayout
                        android:focusable="true"
                        android:clickable="true"
                        android:onClick="changeAgentPassword"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TableRow
                            android:gravity="center"
                            android:layout_weight="1">
                            <ImageView
                                android:contentDescription="@string/na"
                                android:src="@drawable/password"
                                android:scaleType="centerInside"
                                />
                        </TableRow>

                        <TableRow
                            android:gravity="center"
                            android:layout_weight="1">
                            <TextView
                                android:textAlignment="center"
                                android:layout_weight="1"
                                android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                                android:text="@string/change_password"
                                />
                        </TableRow>
                    </TableLayout>

            </androidx.cardview.widget.CardView>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView
                app:cardCornerRadius="4dp"
                android:padding="10dp"
                android:layout_marginTop="0dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_margin="2dp"
                >

                    <TableLayout
                        android:focusable="true"
                        android:clickable="true"
                        android:onClick="viewCollections"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TableRow
                            android:gravity="center"
                            android:layout_weight="1">
                            <ImageView
                                android:contentDescription="@string/na"

                                android:src="@drawable/makusanyo"
                                android:scaleType="centerInside"
                                />
                        </TableRow>

                        <TableRow
                            android:gravity="center"
                            android:layout_weight="1">
                            <TextView
                                android:textAlignment="center"
                                android:layout_weight="1"
                                android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                                android:text="@string/makusanyo_yangu"
                                />
                        </TableRow>
                    </TableLayout>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardCornerRadius="4dp"
                android:padding="10dp"
                android:layout_marginTop="0dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_margin="2dp"
                >

                <TableLayout
                    android:focusable="true"
                    android:clickable="true"
                    android:onClick="viewSuperAgentReport"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <ImageView
                            android:contentDescription="@string/na"

                            android:src="@drawable/money_bundle"
                            android:scaleType="centerInside"
                            />
                    </TableRow>

                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="1"
                            android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                            android:text="@string/taarifa_wakala_mkuu"
                            />
                    </TableRow>
                </TableLayout>

            </androidx.cardview.widget.CardView>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView
                app:cardCornerRadius="4dp"
                android:padding="10dp"
                android:layout_marginTop="0dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_margin="2dp"
                >

                <TableLayout
                    android:focusable="true"
                    android:clickable="true"
                    android:onClick="payServiceFee"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <ImageView
                            android:contentDescription="@string/na"

                            android:src="@drawable/pay_cash"
                            android:scaleType="centerInside"
                            />
                    </TableRow>

                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="1"
                            android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                            android:text="@string/lipia_mfumo"
                            />
                    </TableRow>
                </TableLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                app:cardCornerRadius="4dp"
                android:padding="10dp"
                android:layout_marginTop="0dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_margin="2dp"
                >

                <TableLayout
                    android:focusable="true"
                    android:clickable="true"
                    android:onClick="getHelp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <ImageView
                            android:contentDescription="@string/na"

                            android:src="@drawable/support"
                            android:scaleType="centerInside"
                            />
                    </TableRow>

                    <TableRow
                        android:gravity="center"
                        android:layout_weight="1">
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="1"
                            android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                            android:text="@string/pata_msaada"
                            />
                    </TableRow>
                </TableLayout>
            </androidx.cardview.widget.CardView>
        </TableRow>
        <TableRow
            >
            <TextView
                android:padding="8dp"
                android:gravity="center"
                android:background="@color/whiteColor"
                android:id="@+id/bill_view"
                android:layout_weight="1"
                android:text=""
                android:textColor="#000"
                />

        </TableRow>
    </TableLayout>
        <androidx.cardview.widget.CardView
            android:layout_height="wrap_content"
            app:cardCornerRadius="4dp"
            android:padding="10dp"
            android:layout_marginTop="0dp"
            android:layout_width="match_parent"
            android:layout_margin="2dp"
            android:layout_alignParentBottom="true"
            >

            <TableLayout
                android:focusable="true"
                android:clickable="true"
                android:onClick="logoutUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                    android:paddingStart="20dp"
                    android:paddingLeft="20dp"
                    android:paddingEnd="20dp"
                    android:paddingRight="20dp"
                    android:gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:contentDescription="@string/na"
                        android:src="@drawable/logout_ic"
                        android:scaleType="centerInside"
                        />
                </TableRow>

                <TableRow
                    android:gravity="center"
                    android:layout_weight="1">
                    <TextView
                        android:textColor="@color/redColor"
                        android:textAlignment="center"
                        android:layout_weight="1"
                        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"
                        android:text="@string/logout"
                        />
                </TableRow>
            </TableLayout>

        </androidx.cardview.widget.CardView>

</tz.co.xhcodes.com.CustomRelativeLayout>